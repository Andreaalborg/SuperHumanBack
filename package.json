{
  "name": "superhuman-backend",
  "version": "1.0.0",
  "description": "Backend API for SuperHuman app",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "dev:mock": "MOCK_MODE=true nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "test": "jest",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:create": "typeorm-ts-node-commonjs migration:create",
    "migration:generate": "typeorm-ts-node-commonjs -d src/config/database.ts migration:generate",
    "migration:run": "typeorm-ts-node-commonjs -d src/config/database.ts migration:run",
    "migration:revert": "typeorm-ts-node-commonjs -d src/config/database.ts migration:revert",
    "db:setup": "node scripts/setup-database.js",
    "db:reset": "node scripts/setup-database.js && npm run migration:run"
  },
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "openai": "^4.28.0",
    "pg": "^8.11.3",
    "react-native-share": "^12.0.11",
    "react-native-view-shot": "^4.0.3",
    "redis": "^4.6.11",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
